#include<stdio.h>
int main()
	{
		printf("Nama : Muh. Faisal Pratama \n");
		printf("NIM : F1B019092 \n");
		printf("Kelompok : 19 \n\n");
		
		int x[4],i;
		for (i=0; i<=4; i++)
			{
				printf("Masukkan nilai %i : ",i+1);
				scanf("%i",&x[i]);
			}
		printf("\n");
		for (i=0; i<=4; i++)
			{
				printf("Nilai ke-%i = %i \n",i+1,x[i]);
			}
		printf("\n");
		return 0;
	}
