#include<stdio.h>
int main()
	{
		printf("Nama : Muh faisal pratama \n");
		printf("NIM : F1B019092 \n");
		printf("Kelompok : 19 \n\n");
		
		int i;
		char nama[30]={'M','u','h',' ','f','a','i','s','a','l',' ','p','r','a','t','a', 'm','a'};
		char nama2[30]="Muh faisalpratama";
		printf("Array per karakter = ");
		
		for (i=0; i<=29; i++)
			{
				printf("%c",nama[i]);
			}
		printf("\n");
		printf("Array string = %s \n\n", nama2);
	return 0;
	}
